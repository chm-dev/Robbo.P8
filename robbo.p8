--map code
mapx=0
new_mapx=0
mapy=0
new_mapy=0
function map_setup()
--timers
timer=0
period=30  -- 30 = 1 sec

--map tile settings
 wall={22,23,24,25,26,27,28,29,20,21,32}
 pushable={16,20,21,32}
 key={19}
 door={22}
 battery={18}
 anim1={23,33,35,37,39,41,43,45}
 anim2={24,34,36,38,40,42,44,46}
 lose={}
 win={33,34}
end

function update_map()
	if (timer%10==0) toggle_tiles()
end



function draw_map()
	map(0,0,0,0,16,31)
end

function pan_camera()
--new_mapx=flr(p.x/16)*16
--new_mapy=flr(p.y/14)*14
printh(mapy..", "..p.y)
if (p.y-new_mapy > 10) new_mapy+=6
if (p.y-new_mapy < 5) new_mapy-=6

new_mapy=mid(0,new_mapy,17)
	if (mapx ~= new_mapx) then
	 if(mapx > new_mapx) then
		 mapx-=0.5
	 else 
 	 mapx+=0.5
	 end
	end
 
 if (mapy ~= new_mapy) then
	 if(mapy > new_mapy) then 
	 	mapy-=0.125
			mapy=mid(new_mapy,mapy,17)
	 else 
	 	mapy+=0.125
			mapy=mid(0,mapy,new_mapy)
		end 

	end

 camera(mapx*8,mapy*8)


end


function is_tile(tile_type,x,y)
 tile=mget(x,y)
 --has_flag=fget(tile,tile_type)
 for i=1,#tile_type do
 	if (tile==tile_type[i]) return true
 end
 
 return false
end

function can_move(x,y)
	return not is_tile(wall,x,y)
end

function can_push(x,y)
	if (mget(x,y) == 0) then
	 return true
	else 
		return false
	end
	
end

function delete_tile(x,y)
 mset(x,y,0)
end

function swap_tile(x,y)
 tile=mget(x,y)
 mset(x,y,tile+1)
end

function unswap_tile(x,y)
 tile=mget(x,y)
 mset(x,y,tile-1)
end

function get_key(x,y)
	p.keys+=1
	delete_tile(x,y)
 sfx(2)
end

function get_battery(x,y)
	p.battery+=9
	delete_tile(x,y)
	sfx(5)
end

function open_door(x,y)
 p.keys-=1
 delete_tile(x,y)
 sfx(3)
end

function push_pushable(x,y)
	local dx=0
	local dy=0
	if (p.direction == "➡️") dx=1
	if (p.direction == "⬅️") dx=-1
	if (p.direction == "⬇️") dy=1
	if (p.direction == "⬆️") dy=-1
 if (can_push(x+dx,y+dy)) then
		tile=	mget(x,y)
		delete_tile(x,y)
		mset(x+dx,y+dy,tile)
		sfx(6)
	end
 
end
